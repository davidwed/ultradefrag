-- this is the ultradefag console interface build options
name        = "udefrag"
target_type = "console"

-- getopt* files will be copied by build.cmd command to obj\console directory

src = { "defrag.c", "getopt.c", "getopt1.c", "options.c", "map.c" }
rc = { "defrag.rc" }

libs = { "kernel32", "msvcrt" }
adlibs ={ "$(UDEFRAG_LIB_PATH)\\udefrag" }

umentry = "main"
