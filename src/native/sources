!IF "$(NT4_TARGET)" == "true"
TARGETNAME=defrag_native_nt4
!ELSE
TARGETNAME=defrag_native
!ENDIF
TARGETPATH=obj
TARGETTYPE=PROGRAM

!IF "$(NT4_TARGET)" == "true"
USER_C_FLAGS=/DUSE_WINDDK /DNT4_TARGET
RCOPTIONS=/d NT4_TARGET
!ELSE
USER_C_FLAGS=/DUSE_WINDDK
!ENDIF

SOURCES=defrag_native.c misc.c keytrans.c registry.c stdio.c sys.c defrag_native.rc

USE_NTDLL=1

UMTYPE=nt
UMBASE=0x0400000
UMENTRYABS=NtProcessStartup
